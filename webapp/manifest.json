{
	"_version": "1.5.0",
	"sap.app": {
		"id": "pnp.blockandreplace",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"LOY_BLOCKREPLACE_SRV": {
				"uri": "/sap/opu/odata/pnp/LOY_BLOCKREPLACE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/LOY_BLOCKREPLACE_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "pnp.blockandreplace.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			},
			"components": {
				"pnp.onetimepin": {
					"lazy": false
				}
			}
		},
		"resourceRoots": {
			"pnp.onetimepin": "../sap/bc/ui5_ui5/pnp/onetimepin/"
		},
		"componentUsages": {
			"OneTimePin": {
				"name": "pnp.onetimepin",
				"settings": {},
				"componentData": {
					"sOTPPurpose": "Block & Replace"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pnp.blockandreplace.i18n.i18n"
				}
			},
			"OptionsModel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/Options.json"
			},
			"LoyaltyModel": {
				"uri": "/sap/opu/odata/pnp/LOY_BLOCKREPLACE_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "LOY_BLOCKREPLACE_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "pnp.blockandreplace.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "BlockReplace",
				"pattern": "",
				"target": [
					"BlockReplace"
				]
			}],
			"targets": {
				"BlockReplace": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "BlockReplace"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/pnp/blockreplace/webapp",
		"_version": "1.1.0"
	}
}